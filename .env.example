# Server Configuration
PORT=3000

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# ElevenLabs TTS Configuration (Optional - will fallback to Twilio TTS if not provided)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=your_voice_id_here

# Twilio Configuration (Not needed in code but useful for reference)
# TWILIO_ACCOUNT_SID=your_twilio_account_sid
# TWILIO_AUTH_TOKEN=your_twilio_auth_token
# TWILIO_PHONE_NUMBER=your_twilio_phone_number

# N8N Webhook URL (Production webhook for both appointments and messages)
N8N_WEBHOOK_URL=https://ahadandco.app.n8n.cloud/webhook/ahad

# Cal.com API Configuration (for calendar availability and booking)
CALCOM_API_KEY=your_calcom_api_key_here
CALCOM_EVENT_TYPE_ID=your_event_type_id_here
CALCOM_TIMEZONE=America/New_York
